import java.util.Scanner;
import java.lang.String;
public class Main {
    public static void main(String[] args) {
        Scanner o = new Scanner(System.in);
        int x1 = o.nextInt();

        double x2 = o.nextDouble();

        o.nextLine(); //this is used so that string intake doesn't get escaped.

        String x3 = o.nextLine();


        System.out.println(x1);
        System.out.println(x2);
        System.out.println(x3);

        if(1<2){
            System.out.println("Within 1st if");
        }
        if(2==6){
            System.out.println("Within 2nd if");
        }
        else{
            System.out.println("Within else")
        }

        /* Here, the output will be:
                    Within 1st if
                    Within else
        We are not supposed to do ifs repeatedly one after another unless it's utterly necessary */

        switch(n){
            case 1: //name of case is 1
                //body
                break;
            case 2:
                //body
                break;
        }

        //name of case depends on the value of n. it can be 1, 0 or 1000, 78 etc.

    }
}
